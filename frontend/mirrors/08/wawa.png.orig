    <script src="https://gh5tc-cos.mp.sztv.com.cn/ysz_web_static/plugins/clklog/v3/autotrack.js?project=dyxc&token=ddf51db3-7c99-1310-a44f-79feb7b63c69"></script>
    <script>
        document.getElementById("header").style.display = yszSdkUtils.LoginUtil.checkSilent().env == "Y" ? 'none':'block';
    </script>
    <script>
        const news = [];
        async function initPage(){
            const getTopNews = async () => {
                const {returnData} = await yszSdkUtils.HttpUtil.ajax({
                    url: `https://apix.scms.sztv.com.cn/api/com/article/getArticleList?tenantId=ysz&banner=0&catalogId=12078&page=1&pageSize=3`,
                    method: "GET",
                    auth:true,
                });
                if(returnData && returnData.news) {
                    returnData.news.forEach(item => {
                        news.push({
                            title: item.title,
                            url: item.redirectUrl,
                            type: "top"
                        });
                    });
                }
            };
            const getHotNewsList = async () => {
                const {returnData} = await yszSdkUtils.HttpUtil.ajax({
                    url: `https://apix.scms.sztv.com.cn/api/com/article/getArticleList?catalogId=6552&tenantid=ysz&tenantId=ysz&pageSize=20&banner=1&page=1&extendtype=1&specialtype=1`,
                    method: "GET",
                    auth:true,
                });
                if(returnData && returnData.news) {
                    returnData.news.forEach(item => {
                        news.push({
                            title: item.title,
                            url: item.redirectUrl,
                            type: "list"
                        });
                    });
                }
            };
            await getTopNews();
            await getHotNewsList();
            const newsList = document.querySelector('.news-list-more-list');
            topNews = news.filter(item => item.type === 'top');
            news.forEach((item, index) => {
                const articleItem = document.createElement('div');
                articleItem.className = 'item_article';
                // 根据类型添加不同的类名
                const isTop = item.type === 'top';
                articleItem.innerHTML = `
                    <a href="${item.url}" target="_blank">
                        <div class="item_rankingLi">
                            <div class="item_rankingLiInner">
                                ${isTop ? '<img src="https://www.sztv.com.cn/ysz/upload/Image/mrtp/2025/06/09/119da84f25c248fe8e103a0b89ee9823.png" class="item_rankingTopIcon" alt="">' : `<div class="item_index rand_${index - topNews.length + 1}">${index - topNews.length + 1}</div>`}
                                <div class="item_titleAndIcon">
                                    <div class="item_text">${item.title}</div>
                                </div>
                            </div>
                        </div>
                    </a>
                `;
                newsList.appendChild(articleItem);
            });
        }
        initPage();
    </script>
